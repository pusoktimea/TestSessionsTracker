{
  "manifest_version":2,

  "name":"Test sessions tracker",
    "background": {
       "persistent": false,
       "scripts": [ "popup.js" ]
    },

  "description": "Test sessions tracker",

  "version": "1.0",

  "browser_action": {

    "default_icon":"icon.png",
              
    "default_popup":"index.html"

  },


      "permissions": [ "activeTab", "tabs",  "http://*/*", 
    "https://*/*" ]

 
}